<%= render :partial => 'spree/admin/shared/product_sub_menu' %>
<%= render :partial => 'spree/admin/shared/product_tabs', :locals => {:current => "Images"} %>
<%= render "spree/shared/error_messages", :target => @image %>

<%= form_for(@product_status, :url => admin_product_product_status_url(@product), :html => { :method => :put, :multipart => true }) do |f| %>
  <table class="basic-table">
    <%= render :partial => "form", :locals => { :f => f } %>
  </table>
  <p class="form-buttons">
    <%= button t("update") %>
    <%= t('or') %> <%= link_to t("cancel"), admin_product_product_status_url(@product) %>
  </p>
<% end %>
